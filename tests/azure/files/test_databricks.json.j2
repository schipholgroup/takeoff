{
  "name": "{{ application_name }}",
  "new_cluster": {
    "spark_version": "4.1.x-scala2.11",
    "spark_conf": {
      "spark.sql.warehouse.dir": "/some_",
      "some.setting": "true"
    },
    "cluster_log_conf": {
      "dbfs": {
        "destination": "dbfs:/mnt/sdh/logs/{{ log_destination }}"
      }
    }
  },
  "some_int": 5,
  "libraries": [
    {
      "jar": "{{ jar_file }}"
    }
  ],
  "spark_jar_task": {
    "main_class_name": "{{ class_name }}",
    "parameters":  {{ parameters | tojson }}
  }
}
