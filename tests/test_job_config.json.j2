{
  "name": "{{ application_name }}",
  "new_cluster": {
    "spark_version": "4.1.x-scala2.11",
    "spark_conf": {
      "spark.sql.warehouse.dir": "/some_",
      "some.setting": "true"
    },
    "cluster_log_conf": {
      "dbfs": {
        "destination": "dbfs:/mnt/sdh/logs/{{ log_destination }}"
      }
    }
  },
  "some_int": 5,
  "libraries": [
    { "egg": "{{ egg_file }}" },
    { "jar": "some.jar" }
  ],
  "spark_python_task": {
    "python_file": "{{ python_file }}",
    "parameters": [{% for p in parameters %}"{{ p }}"{% if not loop.last %},{% endif %}{% endfor %}]
  }
}
